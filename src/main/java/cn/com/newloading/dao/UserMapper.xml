<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="cn.com.newloading.dao.UserDao">
	<select id="findByUsername" parameterType="java.lang.String" resultType="cn.com.newloading.bean.User">
		SELECT * FROM userinfo WHERE username = #{username}
	</select>
</mapper>